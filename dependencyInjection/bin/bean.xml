<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="student" class="com.spring.dependencyInjection.Student">
		<!-- <property name="studentName" value="suman" /> <property name="id" 
			value="1" /> -->

		<constructor-arg name="studentName" value="suman" />
		<constructor-arg name="id" value="1" />
	</bean>

	<bean id="student1" class="com.spring.dependencyInjection.Student">

		<constructor-arg name="id" value="2" />
	</bean>

</beans>