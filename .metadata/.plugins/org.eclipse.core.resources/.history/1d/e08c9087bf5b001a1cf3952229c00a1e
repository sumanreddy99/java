package com.java;

public class AggregationTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        Address address=new Address("79", "Rajeev", "CM", "Perungudi");
		
		System.out.println(address);
		
		Employee employee=new Employee(551, "suman", "suman@live.com", address);
		
		System.out.println("employee"+employee);
		
		// child exist independently of parent class - address(child),employee(parent)
		
		// if employee(parent) object destroyed child object exist
		/*
		 * employee=null;
		 * 
		 * System.out.println("employee"+employee);
		 */
		System.out.println("address"+address);
	}

}
