<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

	<bean	class="com.ioc.di.collectionInject.pojo.CarTest"		id="t">
		<property name="carNames">
		<list>
		<value>audi</value>
		<value>benz</value>
		<value>bmw</value>
		</list>
		</property>
		
		<property name="modalYear">
			<set>
				<value>2013</value>
				<value>2014</value>
				<value>2015</value>
			</set>
		</property>
		
		<property name="brandVersion">
		<map>
		<entry key="audi" value="A234"/>
		<entry key="benz" value="B123"/>
		<entry key="bmw" value="C123"/>
		</map>
		
		
		</property>
		
	</bean>

</beans>